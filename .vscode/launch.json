{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Inference HoVer-Net",
        "type": "python",
        "request": "launch",
        "program": "run_infer.py",
        "console": "integratedTerminal",
        "args": [
            "--nr_types", "5", "--type_info_path", "type_info.json",
            "--batch_size", "5", "--model_mode", "fast",
            "--model_path",  "C:/Users/cervaf/Documents/Logging/hover_net/pretrained_models/hovernet_fast_monusac_type_tf2pytorch.tar",
            "--nr_inference_workers", "4", "--nr_post_proc_workers", "4",
            "wsi",
            "--input_dir", "C:/Users/cervaf/Documents/Logging/hover_net/zarr",
            "--output_dir", "C:/Users/cervaf/Documents/Logging/hover_net/result",
            "--input_mask_dir",  "C:/Users/cervaf/Documents/Logging/hover_net/mask",
        ]
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd" : "${fileDirname}", // run at the location of the runfile
        },
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost",
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ]
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "enter-your-module-name-here",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "django": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}